    "# This is Recursive Function.\n",
    "#factorial of a number.\n",
    "def fact(n):\n",
    "\n",
    "    if(n == 1 or n==0):\n",
    "        return 1\n",
    "    else:\n",
    "     factN = n * fact(n-1)\n",
    "     return factN\n"
   ]
  },
